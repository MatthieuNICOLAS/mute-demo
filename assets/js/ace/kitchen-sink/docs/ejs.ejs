<!DOCTYPE html>
<html>
    <head>
        <title>Cloud9 Rocks!</title>
    </head>
    <body>

    <table class="table">
        <tr>
            <th>Name</th>
            <th>Size</th>
        </tr>
        <% if (!isRoot) ***REMOVED*** %>
            <tr>
                <td><a href="..">..</a></td>
                <td></td></td>
            </tr>
        <% ***REMOVED*** %>
        <% entries.forEach(function(entry) ***REMOVED*** %>
            <tr>
                <td>
                    <span class="glyphicon <%= entry.mime == 'directory' ? 'folder': 'file'%>"></span>
                    <a href="<%= entry.name %>"><%= entry.name %></a>
                </td>
                <td><%= entry.size %></td>
            </tr>
        <% ***REMOVED***) %>
    </table>
    
    </body>
</html>